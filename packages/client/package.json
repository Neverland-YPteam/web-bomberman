{
  "name": "client",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:client": "tsc && vite build --outDir dist/client",
    "build:server": "tsc && vite build --outDir dist/server --ssr src/entry-server.tsx",
    "build": "tsc && npm run build:client && npm run build:server",
    "preview": "vite preview",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "jest",
    "serve": "cross-env NODE_ENV=production node --loader ts-node/esm src/server"
  },
  "dependencies": {
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@mui/material": "5.10.3",
    "@reduxjs/toolkit": "^1.9.0",
    "@types/offscreencanvas": "^2019.7.0",
    "eslint-config-prettier": "8.5.0",
    "nanoid": "4.0.0",
    "notistack": "2.0.8",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "8.0.2",
    "react-router-dom": "6",
    "redux": "4.2.0",
    "redux-thunk": "2.4.1"
  },
  "devDependencies": {
    "@testing-library/react": "13.3.0",
    "@types/compression": "^1.7.2",
    "@types/jest": "28.1.8",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.35.1",
    "@typescript-eslint/parser": "5.35.1",
    "@vitejs/plugin-react": "2.0.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "eslint": "8.23.0",
    "express": "^4.18.2",
    "jest": "28",
    "jest-environment-jsdom": "29.0.1",
    "lefthook": "1.1.1",
    "prettier": "2.7.1",
    "serve-static": "^1.15.0",
    "ts-jest": "28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "4.8.2",
    "vite": "3.0.7"
  },
  "license": "MIT"
}
